<Window x:Class="HelloWorld.NavigatorWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="NavigatorWindow" Height="300" Width="300">
    <Window.Background>
        <ImageBrush ImageSource="Images/wpf-world.png" />
    </Window.Background>
    <StackPanel>
    <TextBlock Margin="10,20">Enter an address: (<Run Foreground="Red" FontWeight="Bold">Don't forget http://</Run>)</TextBlock>
    <TextBox x:Name="uxURI"></TextBox>
    <Button x:Name="uxGo" Click="uxGo_Click" Background="BlanchedAlmond">Go</Button>
    
    </StackPanel>

</Window>
